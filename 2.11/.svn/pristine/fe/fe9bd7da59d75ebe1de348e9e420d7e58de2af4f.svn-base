            <SolidColorBrush x:Key="AccentBrush" Color="{telerik:Windows8Resource ResourceKey=AccentBrush}" />

            <!--<Style TargetType="GridSplitter">
                <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
