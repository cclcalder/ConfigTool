            </Style>-->

            <Style x:Key="RectangleOverlayStyle" TargetType="Rectangle">
                <Setter Property="Fill" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
                <Setter Property="Opacity" Value="0.05"/>
            </Style>

            <Style x:Key="MenuItemCheckBoxStyle" TargetType="{x:Type MenuItem}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type MenuItem}">
                            <Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="False">
                                <Grid x:Name="Grid">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition MinWidth="25" SharedSizeGroup="MenuItemIconColumnGroup" Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition SharedSizeGroup="MenuItemIGTColumnGroup" Width="Auto"/>
                                        <ColumnDefinition Width="14"/>
                                    </Grid.ColumnDefinitions>
                                    <Path x:Name="CheckPanel" Data="M0,2L0,4.8 2.5,7.4 7.1,2.8 7.1,0 2.5,4.6z" Fill="{TemplateBinding Foreground}" FlowDirection="LeftToRight" Margin="10,0" Visibility="Hidden" VerticalAlignment="Center"/>
                                    <ContentPresenter x:Name="ContentPresenter" Grid.Column="1" ContentSource="Header" RecognizesAccessKey="True" />
                                    <TextBlock x:Name="InputGestureText" Grid.Column="2" DockPanel.Dock="Right" Margin="5,2,0,2" Text="{TemplateBinding InputGestureText}"/>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter Property="Visibility" TargetName="CheckPanel" Value="Visible"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <HierarchicalDataTemplate x:Key="RadTreeViewItemTemplate" ItemsSource="{Binding Children, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">

                <StackPanel Orientation="Horizontal"  >

                    <TextBlock Text="{Binding Name}"
        									Background="{Binding StringBackground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" />
                </StackPanel>

            </HierarchicalDataTemplate>

            <HierarchicalDataTemplate x:Key="ParentTreeViewItem"  
                                  ItemsSource="{Binding Children, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="{Binding Name}"
                           Background="{Binding StringBackground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" />
                </StackPanel>

            </HierarchicalDataTemplate>

            <DataTemplate x:Key="ParentTreeViewNode">

                <StackPanel Orientation="Horizontal">
                    <RadioButton Visibility="Visible" />
                    <TextBlock Text="{Binding Name}"
                           Background="{Binding StringBackground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" />
                </StackPanel>
            </DataTemplate>


            <converters:InverseBooleanConverter x:Key="InverseConverter" />
            <converters1:BoolToToggleStateConverter x:Key="BoolToToggleStateConv" />
            <converters1:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
            <converters1:IsNullConverter x:Key="IsNullConverter"/>
            <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
            <converters1:TypeToStyleConverter x:Key="TypeToStyleConverter" />
            <wpf:ImageUrlConverter x:Key="imageUrlConverter" />

            <Style x:Key="ItemContainerStyle" TargetType="{x:Type telerik:RadTreeViewItem}"
               BasedOn="{StaticResource RadTreeViewItemStyle}">

                <Style.Resources>
                    <Style TargetType="{x:Type CheckBox}" BasedOn="{StaticResource CheckBoxStyle}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="True">
                                <Setter Property="IsHitTestVisible" Value="False"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Style.Resources>

                <Setter Property="Background" Value="White" />

                <Setter Property="CheckState"
                    Value="{Binding IsSelectedBool, Converter={StaticResource BoolToToggleStateConv}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Setter Property="IsExpanded"
                    Value="{Binding IsExpanded, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Setter Property="Tag" Value="{Binding Idx}"  />

                <Style.Triggers>
                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="False"/>
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Foreground" Value="Black" />
                    </MultiDataTrigger>

                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="True"/>
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Foreground" Value="#FFB3B3B3" />
                    </MultiDataTrigger>

                    <DataTrigger Binding="{Binding IsParentNode}" Value="True">
                        <Setter Property="Foreground" Value="OrangeRed" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CustomersIsSingleSelection, RelativeSource={RelativeSource AncestorType={x:Type UserControl}, Mode=FindAncestor}}" Value="True" >
                        <Setter Property="OptionType" Value="None"/>
                    </DataTrigger>

                </Style.Triggers>


            </Style>

            <Style x:Key="ProductItemContainerStyle" TargetType="{x:Type telerik:RadTreeViewItem}"
               BasedOn="{StaticResource RadTreeViewItemStyle}">

                <Style.Resources>
                    <Style TargetType="{x:Type CheckBox}" BasedOn="{StaticResource CheckBoxStyle}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="True">
                                <Setter Property="IsHitTestVisible" Value="False"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Style.Resources>

                <Setter Property="Background" Value="White" />

                <Setter Property="CheckState"
                    Value="{Binding IsSelectedBool, Converter={StaticResource BoolToToggleStateConv}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Setter Property="IsExpanded"
                    Value="{Binding IsExpanded, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />


                <Style.Triggers>

                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="False"/>
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Foreground" Value="Black" />
                    </MultiDataTrigger>

                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding IsReadOnly, ElementName=Ropanel}" Value="True"/>
                        </MultiDataTrigger.Conditions>
                        <Setter Property="Foreground" Value="#FFB3B3B3" />
                    </MultiDataTrigger>

                    <DataTrigger Binding="{Binding IsParentNode}" Value="True">
                        <Setter Property="Foreground" Value="OrangeRed" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding CustomersIsSingleSelection, RelativeSource={RelativeSource AncestorType={x:Type UserControl}, Mode=FindAncestor}}" Value="True" >
                        <Setter Property="OptionType" Value="None"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding IsDelisted}" Value="True">
                        <Setter Property="Foreground" Value="Orange" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding IsDelisted}" Value="{x:Null}">
                        <Setter Property="Foreground" Value="CornflowerBlue" />
                    </DataTrigger>



                </Style.Triggers>

            </Style>

            <Style x:Key="ReadOnlyContainerStyle" TargetType="{x:Type telerik:RadTreeViewItem}"
               BasedOn="{StaticResource RadTreeViewItemStyle}">

                <Style.Resources>
                    <Style TargetType="{x:Type CheckBox}" BasedOn="{StaticResource CheckBoxStyle}">
                        <Setter Property="IsHitTestVisible" Value="False"/>
                    </Style>
                </Style.Resources>

                <Setter Property="Background" Value="White" />

                <Setter Property="CheckState"
                    Value="{Binding IsSelectedBool, Converter={StaticResource BoolToToggleStateConv}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Setter Property="IsExpanded"
                    Value="{Binding IsExpanded, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <Setter Property="Foreground" Value="#FFB3B3B3" />

            </Style>

            <controls:AutoScrollingListView x:Key="ListboxToolTipTemplate" Height="auto" MaxHeight="500" Width="200" AlternationCount="2"
                                            ScrollViewer.HorizontalScrollBarVisibility="Disabled"               
                                            >
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid ToolTip="{Binding Value}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="{Binding Header}" FontSize="11" TextWrapping="Wrap" FontWeight="Bold" />
                            <TextBlock Grid.Row="1" Text="{Binding Value}" FontSize="12" TextWrapping="Wrap" />
                        </Grid>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </controls:AutoScrollingListView>

            <ListView x:Key="ListViewTemplate" Height="auto" MaxHeight="500" Width="250" BorderBrush="Transparent" BorderThickness="0" > 
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid ToolTip="{Binding Value}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="{Binding Header}" MaxWidth="240" FontSize="11" TextWrapping="Wrap" FontWeight="Bold"/>
                            <TextBlock Grid.Row="1" Text="{Binding Value}" MaxWidth="240" FontSize="12" TextWrapping="Wrap"/>
                        </Grid>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListView>


            <controls:AutoScrollingListView x:Key="ListboxToolTipTemplate2" Height="auto" MaxHeight="500" Width="200" ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid ToolTip="{Binding Value}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Text="{Binding Header}" FontSize="11" TextWrapping="Wrap" FontWeight="Bold" />
                            <TextBlock Grid.Row="1" Text="{Binding Value}" FontSize="12" TextWrapping="Wrap" />
                        </Grid>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </controls:AutoScrollingListView>


            <Grid x:Key="CommentIndicator">
                <ed:RegularPolygon Fill="Red" Height="8.035" InnerRadius="1" PointCount="3" Stretch="None" Stroke="Transparent" Width="13.254"
                                   RenderTransformOrigin="0,0" HorizontalAlignment="Right" Margin="0,-6.248,-10.096,0" VerticalAlignment="Top">
                    <ed:RegularPolygon.RenderTransform>
                        <TransformGroup>
                            <RotateTransform Angle="45" />
                        </TransformGroup>
                    </ed:RegularPolygon.RenderTransform>
                </ed:RegularPolygon>
            </Grid>


          
        </ResourceDictionary>

    </Application.Resources>
</Application>