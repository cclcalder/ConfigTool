<pages:PageBase
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	  xmlns:pages="clr-namespace:WPF.Pages"
	  xmlns:cellsGrid="clr-namespace:Exceedra.CellsGrid;assembly=Exceedra.Controls"
	  x:Class="WPF.Pages.MyActionsDashboard.MyActionsDashboard"
      mc:Ignorable="d"
      d:DesignHeight="800" d:DesignWidth="800"
      Title="My actions dashboard"
      x:Name="MyActionsDashboardView">

    <Page.Resources>
        
    </Page.Resources>

    <Grid DataContext="{Binding ViewModel, ElementName=MyActionsDashboardView}">
        <cellsGrid:CellsGrid ViewModel="{Binding CellsGrid}"/>
    </Grid>
</pages:PageBase>
